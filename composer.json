{
    "name": "dinamiko/dk-pdf",
    "description": "WordPress to PDF made easy.",
    "type": "wordpress-plugin",
    "license": "GPL-3.0+",
    "homepage": "http://wp.dinamiko.com/demos/dkpdf",
    "authors": [
        {
            "name": "Emili Castells",
            "email": "emili.castells@dinamiko.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "mpdf/mpdf": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Dinamiko\\DKPDF\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "squizlabs/php_codesniffer": "^3.0",
        "wimg/php-compatibility": "^8.0",
        "wp-coding-standards/wpcs": "^0.14",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Dinamiko\\DKPDF\\Tests\\Unit\\": "tests/php/unit"
        }
    }
}
